{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "jsx": "react",
    "lib": ["ESNext", "DOM", "WebWorker"],
    "module": "ESNext",
    "moduleResolution": "node",
    "paths": {
      "react": ["node_modules/preact/compat"],
      "react-dom/test-utils": ["node_modules/preact/test-utils"],
      "react-dom": ["node_modules/preact/compat"]
    },
    "strict": true,
    "target": "ES2019"
  }
}
