{
  "name": "mitdemokrati-pwa",
  "version": "0.1.0",
  "browser": "./dist/index.html",
  "server": "./dist/prefetcher.js",
  "author": "Andreas Goodstein <andreasgoodstein@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "parcel ./src/index.html",
    "build": "parcel build --target=browser ./src/index.html --no-source-maps",
    "build:prefetcher": "parcel build --target=server ./src/prefetcher.ts",
    "lint": "npm-run-all -p  lint:prettier lint:es lint:style",
    "lint:es": "eslint . --ext .ts,.tsx",
    "lint:prettier": "prettier -c .",
    "lint:style": "stylelint **/*.less",
    "test": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm-run-all -p build test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint",
      "prettier -c"
    ],
    "*.less": "stylelint"
  },
  "source": [
    "./src/prefetcher.ts",
    "./src/index.html"
  ],
  "targets": {
    "browser": {
      "context": "browser",
      "engines": {
        "browsers": [
          "last 3 versions",
          "not dead",
          "not ie <= 11",
          "not < 1%"
        ]
      }
    },
    "server": {
      "context": "node",
      "engines": {
        "node": "12.x"
      }
    }
  },
  "dependencies": {
    "axios": "^0.19.2",
    "axios-retry": "^3.1.8",
    "lz-string": "^1.4.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.2.1",
    "@testing-library/react-hooks": "^3.3.0",
    "@types/jest": "^25.2.3",
    "@types/lz-string": "^1.3.34",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "jest-transform-stub": "^2.0.0",
    "less": "^3.11.3",
    "lint-staged": "^10.2.9",
    "npm-run-all": "^4.1.5",
    "parcel": "2.0.0-alpha.3.2",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "stylelint": "^13.6.0",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  }
}
